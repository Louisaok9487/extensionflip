{
  "manifest_version": 3,
  "name": "NZ Flipping Assistant Pro",
  "version": "1.5",
"icons": {  "48": "icons/icon48.png"},
  "permissions": ["sidePanel", "activeTab", "scripting", "downloads"],
"host_permissions": [
  "*://*.facebook.com/*",
  "*://*.fbcdn.net/*",
  "*://*.trademe.co.nz/*"
],
  "background": { "service_worker": "background.js" },
  "action": { "default_title": "Open Flipping Assistant" },
  "side_panel": { "default_path": "panel.html" }
}